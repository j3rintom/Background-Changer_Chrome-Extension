{
    "manifest_version": 3,
    "name": "Background Changer",
    "version": "1.0.0",
    "description": "Does stuff with the background.",
    "icons": {
        "128" : "128x128.png",
        "48" :"48x48.png",
        "16" : "16x16.png"
    },
    "action": {
      "default_icon": "16x16.png",
      "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab"
    ]
  }